{
  "name": "car-crm-server",
  "version": "1.0.0",
  "description": "CRM for car services",
  "main": "src/server.js",
  "author": "Bradford Lamson-Scribner & Alex James",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bradford-hamilton/car-crm-server.git"
  },
  "engines": {
    "node": "7.9.0"
  },
  "scripts": {
    "start": "node dist/src/server.js",
    "build": "npm run clean && babel . -d dist --ignore node_modules, test",
    "clean": "rm -rf dist && mkdir dist",
    "test": "npm run lint && npm run tests",
    "lint": "./node_modules/.bin/eslint src/**/*.js db/**/*.js",
    "tests": "NODE_ENV=test mocha -R nyan --compilers js:babel-register $(find test -name '*.test.js')"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "axios": "0.16.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.17.1",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "chai-http": "3.0.0",
    "cookie-parser": "1.4.3",
    "dotenv": "4.0.0",
    "express": "4.15.2",
    "jsonwebtoken": "7.4.0",
    "knex": "0.12.9",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "morgan": "1.8.1",
    "nodemailer": "4.0.1",
    "nodemon": "1.11.0",
    "path": "0.12.7",
    "pg": "6.1.5",
    "sinon": "2.1.0",
    "sinon-chai": "2.9.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "3.3.0"
  }
}
